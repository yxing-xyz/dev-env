[core]
	safecrlf = warn
	autocrlf = false
	fileMode = true
[init]
	defaultBranch = main
[pull]
	rebase = true
[credential]
	helper = store
[user]
	email = yxing.xyz@gmail.com
	name = Ethan
[oh-my-zsh]
	hide-dirty = 1
[alias]
	dlog = "-c diff.external='difft --display inline' log -p --ext-diff"
	dshow = "-c diff.external='difft --display inline' show HEAD --ext-diff"
[diff]
        external = difft --display inline
